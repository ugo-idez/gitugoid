;
; BIND reverse data file for local loopback interface
;
$TTL	604800
$ORIGIN 60.168.192.in-addr.arpa.
@	IN	SOA	dnsm.domaine.lan. root.dnsm.domaine.lan. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;


@	IN	NS	dnsm.domaine.lan.


139	IN	PTR	dnsm.domaine.lan.
140	IN	PTR	xp.
142	IN	PTR	dnse.domaine.lan.


www	CNAME	dnsm.domaine.lan.
